
<% if @saved %>

	<%= render :template => 'sub_groups/show' %>
	$.noticeAdd({ text: "<%= "Group #{t(:update_success)}" %>.", stayTime: 6000 });
	$('.context-menu-selection').html('<%= escape_javascript(render('row_inner', :group => @subgroup))%>');
	$('.context-menu-selection td.time-entry').humaneDates();

<% else %>

	$.noticeAdd({ text: "<%= escape_javascript(render('shared/error_messages', :target => @subgroup)) %>", stay: true });

<% end %>
